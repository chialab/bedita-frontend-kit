{
    "name": "chialab/frontend-kit",
    "description": "BEdita 4 plugin to help with development of websites.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "bedita/core": "^4.2.0",
        "cakephp/cakephp": "^3.8",
        "wyrihaximus/twig-view": "^4.3",
        "chialab/rna-cakephp": "^0.2.1",
        "bedita/i18n": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Chialab\\FrontendKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chialab\\FrontendKit\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
